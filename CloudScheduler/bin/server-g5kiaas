import sys
from scheduler.g5kiaas.service import *

def print_help():

    print """

    program [port]
        
    """

if __name__ == "__main__":
    
    try:

        port = int(sys.argv[1])

    except:

        print_help()
        sys.exit(1)
        
    start_server(port)
